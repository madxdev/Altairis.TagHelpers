@page
@model Altairis.TagHelpers.DemoApp.Pages.CheckBoxListModel
<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>CheckBoxList</title>
    <style type="text/css">
    ul.checkboxlist {
        margin: 0;
        padding: 0;
        list-style-type: none;
    }
  </style>
</head>
<body>
    <form method="post">
        <checkbox-list asp-for="Input.SelectedValues" asp-items="Model.ListValues" class="checkboxlist"></checkbox-list>
        <p>
            <input type="submit" value="Submit" />
        </p>
    </form>
</body>
</html><p>@Model.Message</p>
